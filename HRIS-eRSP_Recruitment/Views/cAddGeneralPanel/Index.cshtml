<script src="~/Angular/ng-cAddGeneralPanel.js"></script>
<section ng-app="ng_eRSP_App" ng-controller="cAddGeneralPanel_Ctrlr">


    <div class="wrapper border-bottom white-bg page-heading" style="background-color:#507CD1">
        <div class="row">
            <div class="col-lg-7">
                <h2 class="text-white"><b>Add General Panel</b></h2>
            </div>
        </div>
    </div>
    <div class="wrapper wrapper-content animated fadeInRight no-padding">
        <div class="row">
            <div class="col-sm-12 col-xs-12 col-md-3 col-lg-3  m-t-xs pull-left">
                <span class="pull-left font-bold" style="margin-top:9px;margin-right:10px;"><b>Show</b></span>
                <select class="form-control field-select-sm setTableRow" style="width:30%;float:left;" tableref="psb_panel">
                    <option value="8">5</option>
                    <option value="10">10</option>
                    <option value="15">15</option>
                    <option value="20">20</option>
                    <option value="50">50</option>
                    <option value="100">100</option>
                </select>
                <span class="font-bold" style="float:left;margin-top:9px;margin-left:10px;"><b>Entries</b></span>
            </div>
            <div class="col-sm-12 col-xs-12 col-md-2 col-lg-2 m-t-xs pull-right">
                <div class="btn-group pull-left">
                    <button class="btn btn-primary" ng-click="addPanel()">Add General Panel&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span class="caret"></span></button>
                </div>
            </div>
        </div>
        <div class="row no-padding" style="margin-top:-10px!important;">
            <div class="col-lg-12 tabs-container m-b-md">
                <table id="psb_panel" class="table table-striped table-bordered table-hover">
                    <thead>
                        <tr>
                            <th width="9%">USER ID</th>
                            <th width="29%">NAME</th>
                            <th width="23%">HRMPSB ROLE</th>
                            <th width="29%">AGENCY</th>
                            <th width="10%">ACTION</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
    <div class="modal fade" id="add_panel" tabindex="-1" role="dialog" aria-labelledby="modalLabelSmall" aria-hidden="true" data-backdrop="static" data-keyboard="false">

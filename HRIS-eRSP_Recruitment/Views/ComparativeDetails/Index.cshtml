<script src="~/Angular/ng-cComparativeDetails.js"></script>
<section ng-app="ng_eRSP_App" ng-controller="cComparativeDetails_Ctrlr">
    <div class="row wrapper border-bottom white-bg page-heading" style="background-color:#507CD1">
        <div class="col-lg-6">
            <h2 class="text-white"><b>Comparative Details</b></h2>
        </div>
        <div class="col-lg-6 pull-right">
            <button class="btn btn-info pull-right" ng-click="backevent()">Back</button>
        </div>
    </div>
    <div class="wrapper wrapper-content animated fadeIn">
        <div class="ibox-title">
            <div class="row m-b-md">

                <button class="btn btn-info pull-right m-r-sm" ng-click="printComparative()">Print Comparative</button>
                <button class="btn btn-success pull-right m-r-sm" ng-click="prepareEndorsement()">View Enodorsment</button>
                <button id="addToComparativeBtn" class="btn btn-info pull-right m-r-sm" type="button" ng-click="approvedComparativeAll()" title="Click for more action">Add for Endorsement</button>
            </div>
            <div class="row m-t-md">

                <table id="Data_List_Grid" style="width:100%;" class="table table-responsive table-striped table-bordered table-hover">
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th class="no-padding no-margin" width="5%">Item No</th>
                            <th class="no-padding no-margin" width="5%">Gender</th>
                            <th class="no-padding no-margin" width="5%">Education<br>25%</th>
                            <th class="no-padding no-margin" width="5%">Training<br>25%</th>
                            <th class="no-padding no-margin" width="5%">Experience<br>25%</th>
                            <th class="no-padding no-margin" width="5%">Eligibility<br>25%</th>
                            <th class="no-padding no-margin" width="5%">QS<br>100%</th>
                            <th class="no-padding no-margin" width="5%">QS<br>50%</th>
                            <th class="no-padding no-margin" width="5%">Exam<br>100%</th>
                            <th class="no-padding no-margin" width="5%">Exam<br>25%</th>
                            <th class="no-padding no-margin" width="5%">CBI<br />100%</th>
                            <th class="no-padding no-margin" width="5%">CBI<br />25%</th>
                            <th class="no-padding no-margin" width="5%">Over-All<br /> Total</th>
                            <th class="no-padding no-margin" width="5%">Rank</th>
                            <th class="no-padding no-margin" width="5%"></th>
                            <th class="no-padding no-margin" width="4%">Send Email</th>
                            <th class="no-padding no-margin" width="4%">Action</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
    <div class="modal fade" id="prepareEndorsement" tabindex="-1" role="dialog" aria-labelledby="modalLabelSmall" aria-hidden="true" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-md">
            <div class="modal-content animated flipInY with-background">
                <div class="modal-header modal-header-add-edit text-white">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <center><h2 class="modal-title" id="modalLabelSmall"><b>Prepare Endorsement</b></h2></center>
                </div>
                <div class="modal-body">
                    <form id="endorseForm">
                        <div class="row">
                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                    <button data-toggle="dropdown" class="btn btn-success btn-block"><span class="pull-left m-l-sm">ENDORSEMENT DATE:</span><span style="" class="fa fa-calendar pull-right m-r-sm"></span></button>
                                </div>
                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                    <div class="input-group date no-padding no-margins" data-provide="datepicker" style="margin-bottom:-2px!important;">
                                        <input type="text" id="endorse_dt" ng-model="endorse_dt" class="form-control text-center" required include />
                                        <div class="input-group-addon birth_date">
                                            <span class="fa fa-calendar"></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </form>
                    <div class="row">
                        <div class="col-sm-12 col-md-12 col-xs-12 col-lg-12">

                            <h1 class="headerendorse" style="font-size:25px !important;">REPUBLIC OF THE PHILIPPINES</h1>
                            <h2 class="headerendorse" style="font-size:20px !important;">PROVINCE OF DAVAO DE ORO</h2>
                            <br />
                            <h2 class="headerendorse m-t-md" style="font-size:20px !important;">PROVINCIAL GOVERNOR'S OFFICE</h2>
                            <br />
                            <br />
                            <br />
                            <div style="text-align:center !important">
                                {{endorse_date}}
                            </div>
                            <div class="endorsement">
                                <p style="text-align:center !important; font-size:15px;">Endorsement</p>
                                <p class="justified" style="text-align: justify;text-justify: inter-word;max-width:600px;margin:0 auto;text-indent:0">
                                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;The {{applicant}} below who {{is}} vying for the position of <b>{{endorse_position}}</b>, <b>SG-{{endorse_salary_grade}}</b>
                                    with <b>item no. {{endorse_item_no}}</b> at <b>{{endorse_department_name1}}</b> has complied with the minimum requirements and passed the screening
                                    of the Human Resource Merit Promotion and Selection Board (HRMPSB). His/her application is respectfully endorsed to
                                    the Honorable Governor for her final action/appointment:
                                </p>
                            </div>
                            <br />
                            <br />
                            <br />
                            <div>

                                <table class="center">
                                    <tr>
                                        <th width="25%"></th>
                                        <th></th>
                                        <th></th>
                                        <th width="25%"></th>
                                    </tr>
                                    <tbody>
                                        <tr ng-repeat="l in endorse_list">
                                            <td></td>
                                            <td>{{l.applicant_rank}}&nbsp;&nbsp;</td>
                                            <td><b style="text-transform:capitalize !important;">{{l.applicant_name}}</b></td>
                                            <td></td>
                                        </tr>
                                    </tbody>
                                </table>
                                @*<div ng-repeat="l in endorse_list">{{l.applicant_rank}}&nbsp;&nbsp;-&nbsp;&nbsp;<b style="text-transform:capitalize !important;">{{l.applicant_name}}</b></div>*@

                            </div>


                        </div>
                    </div>
                </div>

                <div class="modal-footer">
                    <button class="btn btn-info pull-right" ng-click="printEndorsement()"><span id="" class="fa fa-arrow-right"></span>&nbsp;&nbsp;Submit</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="modal_print_preview" tabindex="-1" role="dialog" aria-labelledby="modalLabelSmall" aria-hidden="true">
        <div class="modal-dialog modal-lg" style="width:90%!important;">

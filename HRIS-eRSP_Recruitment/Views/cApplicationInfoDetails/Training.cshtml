<section ng-app="ng_eRSP_App" ng-controller="cAppInfoLndDetail_Ctrlr">
    <!-- The modal -->
    <div class="modal fade" id="lnd_dtl_modal" tabindex="-1" role="dialog" aria-labelledby="modalLabelSmall" aria-hidden="true" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-md">
            <div class="modal-content animated flipInY with-background">
                <div class="modal-header modal-header-add-edit text-white">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h2 class="modal-title" id="modalLabelSmall"><b>LEARNING AND DEVELOPMENT</b></h2>
                </div>

                <form id="ld" ng-model="ld" novalidate>
                    <div class="modal-footer detail">
                        <div class="row" style="">
                            <div class="col-sm-12">
                                <div class="no-padding" style=" float:left;">
                                    <span class="pull-left font-bold" style="">L&D Title:</span>
                                </div>
                                <div style="width:77%; float:right;">
                                    <input type="text" class="form-control" id="learn_devt_title" ng-model="ld.learn_devt_title" required />
                                    <small class="text-center learn_devt_title hidden pull-right" style="font-size:12px;height:15px;color:red">  <i class="fa fa-info-circle"></i>&nbsp;Required field! </small>
                                </div>
                            </div>

                        </div>

                        <div class="row" style="padding-top:5px">
                            <div class="col-sm-6">
                                <div style="float:left;">
                                    <span class="pull-left font-bold" style="margin-right:10px;">From:</span>
                                </div>
                                <div style="width:51.5%; float:right;">
                                    <span class="input-group date no-padding no-margins" data-provide="datepicker" style="margin-bottom:-2px!important;">
                                        <input type="text" name="minDate" id="learn_devt_from" ng-model="el.learn_devt_from" class="form-control text-center dateMinVal" required>
                                        <div class="input-group-addon">
                                            <span class="fa fa-calendar"></span>
                                        </div>
                                    </span>
                                    <small class="text-center learn_devt_from hidden pull-right" style="font-size:12px;height:15px;color:red">  <i class="fa fa-info-circle"></i>&nbsp;Required field! </small>
                                </div>

                            </div>


                            <div class="col-sm-6">
                                <div style=" float:left;">
                                    <span class="pull-left font-bold" style="margin-right:10px">To  :</span>
                                </div>
                                <div style="width:53%; float:right;">
                                    <span class="input-group date no-padding no-margins" data-provide="datepicker" style="margin-bottom:-2px!important;">
                                        <input type="text" name="maxDate" id="learn_devt_to" ng-model="el.learn_devt_to" class="form-control text-center dateMaxVal" required>
                                        <div class="input-group-addon">
                                            <span class="fa fa-calendar"></span>
                                        </div>
                                    </span>
                                    <small class="text-center learn_devt_to hidden pull-right" style="font-size:12px;height:15px;color:red">  <i class="fa fa-info-circle"></i>&nbsp;Required field! </small>
                                </div>

                            </div>
                        </div>
                        <div class="row" style="padding-top:5px">

                            <div class="col-sm-6">
                                <div style="float:left;">
                                    <span class="pull-left font-bold" style="margin-right:10px">Nbr. of Hours:</span>
                                </div>
                                <div style="width:51.5%; float:right;">
                                    <input type="number" class="form-control" id="no_of_hrs" ng-model="ld.no_of_hrs" required />
                                    <small class="text-center no_of_hrs hidden pull-right" style="font-size:12px;height:15px;color:red">  <i class="fa fa-info-circle"></i>&nbsp;Required field! </small>
                                </div>
                            </div>

                            <div class="col-sm-6">
                                <div style="width:30%; float:left;">
                                    <span class="pull-left font-bold" style="margin-right:10px">Devt. Type:</span>
                                </div>
                                <div style="width:70%; float:right;">
                                    <input type="text" class="form-control" id="learn_devt_type" ng-model="ld.learn_devt_type" required />
                                    <small class="text-center learn_devt_type hidden pull-right" style="font-size:12px;height:15px;color:red">  <i class="fa fa-info-circle"></i>&nbsp;Required field! </small>
                                </div>
                            </div>
                        </div>
                        <div class="row" style="padding-top:5px">

                            <div class="col-sm-6">
                                <div style="float:left;">
                                    <span class="pull-left font-bold" style="margin-right:10px">Conducted By:</span>
                                </div>
                                <div style="width:51.5%; float:right;">
                                    <input type="text" class="form-control" id="conducted_by" ng-model="ld.conducted_by" required/>
                                    <small class="text-center conducted_by hidden pull-right" style="font-size:12px;height:15px;color:red">  <i class="fa fa-info-circle"></i>&nbsp;Required field! </small>
                                </div>
                            </div>


                        </div>
                    </div>
                    <div class="modal-footer detail">
                        <div class="row" style="padding-top:5px;">
                            <div class="col-sm-12">
                                <button class=" btn btn-danger font-bold" data-dismiss="modal">
                                    <i class="fa fa-close"></i>&nbsp;&nbsp;Close
                                </button>
                                <button class="btn btn-success font-bold" ng-click="lnd_btn_add(ld)">
                                    Add&nbsp;<i id="add" class="fa fa-plus"></i>
                                </button>
                            </div>

                        </div>
                    </div>

                </form>
            </div>
        </div>
    </div>

    <div class="row wrapper border-bottom white-bg page-heading" style="background-color:#507CD1">
        <div class="col-lg-6">
            <h2 class="text-white"><b>Applicant Trainings</b></h2>
        </div>
        @*<div class="col-lg-6" style="margin-top:12px;">
            <input ng-show="main_edit == false" type="search" class="form-control block" ng-model="search_name" ng-keyup="search_in_list(search_name,'Applicant_List_Grid')" placeholder="Search ....">
        </div>*@
    </div>
    <div class="wrapper wrapper-content animated fadeInRight no-padding">
        <div class="row">
            <div class="col-lg-12">
                <div class="ibox-title">
                    <div class="row">
                        <div class="col-sm-3 no-padding">
                            <div style="width:16%;float:left;margin-bottom:15px;">
                                <span class="pull-left font-bold" style="margin-top:9px;margin-right:10px;"><b>Name:</b></span>
                            </div>
                            <div style="width:82%; float:left;padding-right:0!important;margin-top:0;padding-top:0">
                                <input type="search" class="form-control disable-white" ng-model="applicant_name" disabled>
                            </div>
                        </div>
                        <div class="col-sm-4 no-padding">
                            <div style="width:17%;float:left;margin-bottom:15px;">
                                <span class="pull-left font-bold" style="margin-top:9px;margin-right:10px;"><b>Position:</b></span>
                            </div>
                            <div style="width:80%; float:left">
                                <select class="form-control disable-white" ng-model="item_no" disabled>
                                    <option value="">--Select Here--</option>
                                    <option ng-repeat="l in position_from_publication" value="{{l.item_no}}">{{l.position_long_title}}</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-sm-3 no-padding">
                            <div style="width:30%;float:left;margin-bottom:15px;">
                                <span class="pull-left font-bold" style="margin-top:9px;margin-right:10px;"><b>Appointment:</b></span>
                            </div>
                            <div style="width:70%; float:left;padding-right:0!important;margin-top:0;padding-top:0">
                                <select class="form-control disable-white" ng-model="appointment" ng-click="selectPosition(appointment)" disabled>
                                    <option value="">--Select Here--</option>
                                    <option value="JO">Job Order</option>
                                    <option value="CE">Casual</option>
                                    <option value="RE">Permanent</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-sm-2 no-padding">
                            <div class="btn-group pull-right">
                                <button data-toggle="dropdown" class="btn btn-primary dropdown-toggle">Add/Remove Details <span class="caret"></span></button>
                                <ul class="dropdown-menu">
                                    <li><a ng-click='AddDetails("cApplicantsInfo/Index")'>Back To Applicant List </a></li>
                                    <li><a ng-click='AddDetails("cApplicationInfoDetails/Index")'>Edit Applicants Info</a></li>
                                    <li><a ng-click='AddDetails("cApplicationInfoDetails/Education")'>Add Education</a></li>
                                    <li><a ng-click='AddDetails("cApplicationInfoDetails/Training")'>Add Training  </a></li>
                                    <li><a ng-click='AddDetails("cApplicationInfoDetails/Eligibility")'>Add Eligibility</a></li>
                                    <li><a ng-click='AddDetails("cApplicationInfoDetails/Experience")'>Add Experience</a></li>
                                    <li><a ng-click='AddDetails("cApplicationInfoDetails/Attachements")'>Add Attachements</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="ibox-title">
                    <div class="row no-padding" style="margin-top:-10px!important;">
                        <div class="col-lg-12 tabs-container m-b-md">
                            <table id="appl_lnd_grid" class="table table-striped table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th rowspan="2" width="40%">L&D TITLE</th>
                                        <th colspan="2" width="22%">TRAINING DATE</th>
                                        <th rowspan="2" width="7%">NBR. OF HOURS</th>
                                        <th rowspan="2" width="18%">TYPE</th>
                                        <th rowspan="2" width="15%">CONDUCTED BY</th>
                                        <th rowspan="2" width="7%">ACTION</th>
                                    </tr>
                                    <tr>
                                        <th width="11%">START</th>
                                        <th width="11%">END</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>
</section>

<script src="~/Angular/ng-cGuestUserInfo.js"></script>
<section ng-app="ng_eRSP_App" ng-controller="cGuestUserInfo_Ctrlr">
    <!-- The modal -->
    <div class="modal fade" id="showGuestUserInfo" tabindex="-1" role="dialog" aria-labelledby="modalLabelSmall" aria-hidden="true" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-lg">
            <div class="modal-content animated flipInY with-background">
                <div class="modal-header modal-header-add-edit text-white">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close" ng-click="cancelfetch()">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <center><h2 class="modal-title" id="modalLabelSmall"><b>Add Applicants Examination Result</b></h2></center>
                </div>
                    <div class="modal-footer m-top-n20">
                        <div class="row no-padding" style="margin-top:-10px!important;">
                            <div class="col-lg-12 tabs-container m-b-md">
                                <form ng-model="gu" novalidate>
                                    <div class="row m-top-30">
                                        <div class="col-sm-2">
                                            <div style=" float:left;">
                                                <span class="pull-left font-bold" style="margin-top:9px;margin-right:10px;"><b>User Id:</b></span>
                                            </div>
                                        </div>
                                        <div class="col-sm-3 dt-hdr-rw">
                                            <div style="padding-right:0!important;height:32px!important; margin-bottom:25px!important;">
                                                <input type="text" id="g_user_id" ng-model="gu.g_user_id" class="form-control text-center removeReq" required disabled />
                                                <small class="text-center g_user_id hidden pull-left" style="font-size:12px;height:15px;color:red">  <i class="fa fa-info-circle"></i>&nbsp;Required field! </small>
                                            </div>
                                        </div>

                                    </div>

                                    <div class="row m-top-10">
                                        <div class="col-sm-2">
                                            <div style=" float:left;">
                                                <span class="pull-left font-bold" style="margin-top:9px;margin-right:10px;"><b>Last name:</b></span>
                                            </div>
                                        </div>
                                        <div class="col-sm-10 dt-hdr-rw">

                                            <div style="padding-right:0!important;margin-bottom:25px!important;">
                                                <input type="text" id="last_name" ng-model="gu.last_name" class="form-control text-left removeReq" required />
                                                <small class="text-center last_name hidden pull-left" style="font-size:12px;height:15px;color:red">  <i class="fa fa-info-circle"></i>&nbsp;Required field! </small>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row m-top-10">
                                        <div class="col-sm-2">
                                            <div style=" float:left;">
                                                <span class="pull-left font-bold" style="margin-top:9px;margin-right:10px;"><b>First name:</b></span>
                                            </div>
                                        </div>
                                        <div class="col-sm-10 dt-hdr-rw">

                                            <div style="padding-right:0!important;margin-bottom:25px!important;">
                                                <input type="text" id="first_name" ng-model="gu.first_name" class="form-control text-left removeReq" required />
                                                <small class="text-center first_name hidden pull-left" style="font-size:12px;height:15px;color:red">  <i class="fa fa-info-circle"></i>&nbsp;Required field! </small>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row m-top-10">
                                        <div class="col-sm-2">
                                            <div style="float:left;">
                                                <span class="pull-left font-bold" style="margin-top:9px;margin-right:10px;"><b>Middle name:</b></span>
                                            </div>
                                        </div>
                                        <div class="col-sm-10 dt-hdr-rw">
                                            <div style="margin-bottom:25px!important;">
                                                <input type="text" id="middle_name" ng-model="gu.middle_name" class="form-control text-left removeReq" required />
                                                <small class="text-center middle_name hidden pull-left" style="font-size:12px;height:15px;color:red">  <i class="fa fa-info-circle"></i>&nbsp;Required field! </small>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row m-top-10">
                                        <div class="col-sm-2">
                                            <div style=" float:left;">
                                                <span class="pull-left font-bold" style="margin-top:9px;margin-right:10px;"><b>Civil Status:</b></span>
                                            </div>
                                        </div>
                                        <div class="col-sm-3 dt-hdr-rw">
                                            <div style="margin-bottom:25px!important;">
                                                <select class="form-control removeReqOption" id="civil_status" ng-model="gu.civil_status" required>
                                                    <option value="">--</option>
                                                    <option ng-repeat="l in civil_status" value="{{l.civil_status}}">{{l.civil_status_descr}}</option>
                                                </select>
                                                <small class="text-center civil_status hidden pull-left" style="font-size:12px;height:15px;color:red">  <i class="fa fa-info-circle"></i>&nbsp;Required field! </small>
                                            </div>
                                        </div>
                                        <div class="col-sm-1">
                                            <div style=" float:left;">
                                                <span class="pull-left font-bold" style="margin-top:9px;margin-right:10px;"><b>Gender:</b></span>
                                            </div>
                                        </div>
                                        <div class="col-sm-2 dt-hdr-rw">
                                            <div style="margin-bottom:25px!important;">
                                                <select class="form-control removeReqOption" id="gender" ng-model="gu.gender" required>
                                                    <option value="">--</option>
                                                    <option value="F">Female</option>
                                                    <option value="M">Male</option>
                                                </select>
                                                <small class="text-center gender hidden pull-left" style="font-size:12px;height:15px;color:red">  <i class="fa fa-info-circle"></i>&nbsp;Required field! </small>
                                            </div>
                                        </div>

                                        <div class="col-sm-1">
                                            <div style=" float:left;">
                                                <span class="pull-left font-bold" style="margin-top:9px;margin-right:10px;"><b>Birthdate:</b></span>
                                            </div>
                                        </div>
                                        <div class="col-sm-3 dt-hdr-rw">
                                            <div style="margin-bottom:25px!important;">
                                                <span class="input-group date no-padding no-margins" data-provide="datepicker" style="margin-bottom:-2px!important;">
                                                    <input type="text" id="birth_date" ng-model="gu.birth_date" class="form-control text-center" required />
                                                    <div class="input-group-addon birth_date">
                                                        <span class="fa fa-calendar"></span>
                                                    </div>
                                                </span>
                                                <small class="text-center birth_date hidden pull-right" style="font-size:12px;height:15px;color:red">  <i class="fa fa-info-circle"></i>&nbsp;Required field! </small>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row m-top-10">
                                        <div class="col-sm-2">
                                            <div style="float:left;">
                                                <span class="pull-left font-bold" style="margin-top:9px;margin-right:10px;"><b>Address:</b></span>
                                            </div>
                                        </div>
                                        <div class="col-sm-10 dt-hdr-rw">
                                            <div style="margin-bottom:25px!important;">
                                                <input type="text" id="g_address" ng-model="gu.g_address" class="form-control text-left removeReq" required />
                                                <small class="text-center g_address hidden pull-left" style="font-size:12px;height:15px;color:red">  <i class="fa fa-info-circle"></i>&nbsp;Required field! </small>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row m-top-10">
                                        <div class="col-sm-2">
                                            <div style="float:left;">
                                                <span class="pull-left font-bold" style="margin-top:9px;margin-right:10px;"><b>Agency:</b></span>
                                            </div>
                                        </div>
                                        <div class="col-sm-10 dt-hdr-rw">
                                            <div style="margin-bottom:25px!important;">
                                                <input type="text" id="agency" ng-model="gu.agency" class="form-control text-left removeReq" required />
                                                <small class="text-center agency hidden pull-left" style="font-size:12px;height:15px;color:red">  <i class="fa fa-info-circle"></i>&nbsp;Required field! </small>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row m-top-10">
                                        <div class="col-sm-2">
                                            <div style="float:left;">
                                                <span class="pull-left font-bold" style="margin-top:9px;margin-right:10px;"><b>Position:</b></span>
                                            </div>
                                        </div>
                                        <div class="col-sm-10 dt-hdr-rw">
                                            <div style="margin-bottom:25px!important;">
                                                <input type="text" id="position" ng-model="gu.position" class="form-control text-left removeReq" required />
                                                <small class="text-center position hidden pull-left" style="font-size:12px;height:15px;color:red">  <i class="fa fa-info-circle"></i>&nbsp;Required field! </small>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer detail">
                        <div class="row" style="padding-top:5px;">
                            <div class="col-sm-6 pull-right">
                                <center>
                                    
                                    <button class=" btn btn-danger font-bold pull-right m-right-10" data-dismiss="modal">
                                        Close&nbsp;<i class="fa fa-close"></i>
                                    </button>
                                </center>
                            </div>

                        </div>
                    </div>
            
            </div>
        </div>
    </div>

    <div class="row wrapper border-bottom white-bg page-heading" style="background-color:#507CD1">
        <div class="col-lg-4">
            <h2 class="text-white"><b>Guest Users</b></h2>

        </div>
        <div class="col-lg-6" style="margin-top:12px;">
            <div style="width:100%;float:left;">
                <input type="search" class="form-control headerSearch block" ng-model="search_name" tableref="Guest_Users" placeholder="Search ....">
            </div>

        </div>
    </div>
    <div class="wrapper wrapper-content animated fadeIn">
        <div class="row">
            <div class="col-lg-12">
                <div class="ibox-title">
                    <div class="row">
                        <div class="col-sm-3 m-t-xs pull-left">
                            <span class="pull-left font-bold" style="margin-top:9px;margin-right:10px;"><b>Show</b></span>
                            <select class="form-control field-select-sm setTableRow" style="width:30%;float:left;" ng-model="rowLen" tableref="Guest_Users">
                                <option value="8">5</option>
                                <option value="10">10</option>
                                <option value="15">15</option>
                                <option value="20">20</option>
                                <option value="50">50</option>
                                <option value="100">100</option>
                            </select>
                            <span class="font-bold" style="float:left;margin-top:9px;margin-left:10px;"><b>Entries</b></span>
                        </div>
                        <div class="col-sm-3 m-t-xs">
                            <div style="width:55%;float:left;margin-bottom:15px;">
                                <span class="pull-left font-bold" style="margin-top:9px;margin-right:10px;"><b>Name Start With:</b></span>
                            </div>
                            <div style="width:45%; float:left;padding-right:0!important;margin-top:0;padding-top:0">
                                <select class="form-control" id="start_with" ng-model="start_with" ng-change="selectStartWith(start_with)">
                                    <option value="">--</option>
                                    <option ng-repeat="l in alphabet_list" value="{{l.alpha_name}}">{{l.alpha_name}}</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-sm-2 m-t-xs pull-right">
                            <div class="btn-group pull-left">
                                <button class="btn btn-primary" ng-click="AddGuestUser()">Add Guest User&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span class="caret"></span></button>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
        <div class="ibox-title">
            <div class="row no-padding" style="margin-top:-10px!important;">
                <div class="col-lg-12 tabs-container m-b-md">
                    <table id="Guest_Users" style="width:100%;" class="table table-striped table-bordered table-hover">
                        <thead>
                            <tr>
                                <th width="10%">USER ID</th>
                                <th width="30%">NAME</th>
                                <th width="25%">AGENCY</th>
                                <th width="25%">POSITION</th>
                                <th width="10%">ACTION</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>

    </div>

</section>





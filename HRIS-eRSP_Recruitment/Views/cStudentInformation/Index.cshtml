            width: 35% !important;
        }
        div.responsive_filter_cont_personnel > span {
            width: 70% !important;
            text-align: right !important;
        }
        div.responsive_filter_cont_personnel > select {
            width: 30% !important;
            float: right !important;
        }
    }

    @@media only screen and (max-width:767px) {


        div.responsive_show {
            width: 50% !important;
            float: left !important;
        }

        div.responsive_filter_cont_personnel {
            width: 50% !important;
            float: right !important;
        }

        div.responsive_show > #show_id {
            width: 40% !important;
        }


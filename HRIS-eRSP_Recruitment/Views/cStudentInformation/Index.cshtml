    <div class="modal fade " id="upload_modal" role="dialog" aria-labelledby="modalLabelSmall" aria-hidden="true">
        <div class="modal-dialog modal-sm">
            <div class="modal-content modal-content-lookup">
                <div class="modal-header bg-primary">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h4 class="modal-title">Upload Picture</h4>
                </div>
                <div class="modal-body with-background">
                    <div class="row">
                        <div class="col-lg-12" style="padding:0px 10px 0px 10px">
                            <img id="preview_img" src="{{img_p_only}}" height="250" width="250" style="background-color:white;" />
                            <label id="error_img_msg" class="lbl_required"></label>
                            <script type="text/javascript">
                                function showImage(cont) {

                                    if (cont.files && cont.files[0]) {
                                        var reader = new FileReader();
                                        var fileUpload = $('#input_file_upload').get(0);
                                        var sFileName = $('#input_file_upload').val();
                                        var files = fileUpload.files;
                                        var _validFileExtensions = ["jpg", "jpeg", "bmp", "png"];// ".gif",
                                        var blnValid = false;
                                        var bValid_size = true;
                                        var file_size = 0;
                                        var validation_msg = "";
                                        reader.onload = function (e) {
                                            for (var j = 0; j < _validFileExtensions.length; j++) {
                                                var sCurExtension = _validFileExtensions[j];
                                                var file_extention = sFileName.split('.')[sFileName.split('.').length - 1];
                                                if (file_extention == sCurExtension.toLowerCase()) {
                                                    blnValid = true;
                                                    break;
                                                }
                                            }
                                            $('#preview_img').attr('src', e.target.result);
                                            file_size = (files[0].size / 1024);
                                            if (blnValid == false) {
                                                validation_msg = "Invalid Image Format";
                                            }

                                            if (file_size > 5120) {
                                                validation_msg = validation_msg == "" ? "Large file size is loaded, File size should not be greater than 5 MB" : validation_msg + " and a large file size is loaded, File size should not be greater than 5 MB";
                                                bValid_size = false;
                                            }

                                            if (blnValid && bValid_size) {
                                                $('#error_img_msg').html("");
                                                //alert("1")
                                                angular.element(document.getElementById('section_ng')).scope().SetImageArray("" + e.target.result.split(',')[e.target.result.split(',').length - 1] + "", true);
                                            }
                                            else {
                                                //alert("2")
                                                angular.element(document.getElementById('section_ng')).scope().SetImageArray("", false);
                                                $('#error_img_msg').html(validation_msg);
                                            }
                                        }

                                        reader.readAsDataURL(cont.files[0]);
                                    }
                                    else {

                                        $('#preview_img').click();
                                    }
                                }
                            </script>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">

                    <input type="file" class="form-chat form-control" name="my_file" id="input_file_upload" onchange="showImage(this);" accept="image/png,image/gif,image/jpeg,image/jpg" style="margin-bottom:5px;margin-top:5px;" />
                    <button class="btn btn-success btn-block btn-sm" ng-click="btn_update_image()"> <i class="fa fa-save" id="btn_update_image"></i> Save Image</button>
                </div>
            </div>
        </div>
    </div>



            width: 83% !important;
            float: left;
        }

        div.responsive_btn_add_cont_personnel {
            margin-top: 3px !important;
            width: 17% !important;
            float: left;
        }
    }

    @@media only screen and (max-width:659px) 
    {
        div.responsive_show {
            width: 50% !important;
            float: left !important;
        }

        div.responsive_filter_cont_personnel {
            width: 50% !important;
            float: right !important;
        }

        div.responsive_filter_cont_personnel > select {
            width: 40% !important;
            float: right !important;
        }
        div.responsive_filter_cont_personnel > span {
            width: 60% !important;
            text-align: right !important;
        }
       
    }

    @@media only screen and (max-width:570px) 
    {
        div.responsive_show {
            width: 100% !important;
            float: left !important;
        }

        


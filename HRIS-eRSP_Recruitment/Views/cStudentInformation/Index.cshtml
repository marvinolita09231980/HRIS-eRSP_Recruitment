    }

    @@media only screen and (max-width:1342px) {
        div.responsive_filter_cont_personnel > select {
            width: 40% !important;
        }
    }

    @@media only screen and (max-width:1199px) {
        div.responsive_filter_cont_personnel > select {
            width: 30% !important;
        }

        div.img_container_responsive {
            text-align: center !important;
        }
    }

    @@media only screen and (max-width:1101px) 
    {
        div.responsive_filter_cont_personnel 
